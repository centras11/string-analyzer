{
    "name": "string/analyzer",
    "description": "String Analyzer",
    "keywords": [],
    "type": "project",
    "autoload": {
        "psr-4": {
            "Analyzer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Analyzer\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "symfony/console": "^5.2",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "test": ["@phpunit"]
    }
}
